{
  "name": "azumio",
  "version": "0.0.0",
  "description": "Library to easily access Azumio checkin data.",
  "main": "azumio.js",
  "scripts": {
    "test": "mocha --compilers js:traceurified-mocha test/test*.js",
    "pretest": "[[ ! -z $npm_config_dist ]] && traceur --modules commonjs --script azumio.es6.js --out azumio.dist.js || true",
    "prepublish": "traceur --modules commonjs --script azumio.es6.js --out azumio.dist.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samcday/node-azumio.git"
  },
  "keywords": [
    "azumio",
    "data",
    "api",
    "checkins",
    "heartrate"
  ],
  "author": "Sam Day <me@samcday.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samcday/node-azumio/issues"
  },
  "homepage": "https://github.com/samcday/node-azumio",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "bluebird": "^2.2.2",
    "request": "^2.40.0",
    "tough-cookie": "^0.12.1",
    "traceur": "0.0.55"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.21.4",
    "nock": "^0.44.1",
    "traceurified": "0.0.3",
    "traceurified-mocha": "0.0.1"
  }
}
